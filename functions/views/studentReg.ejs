<html>

<head>
	<title>Student Registration Form</title>

</head>

<body>
	<h3>SCHOOL REGISTRATION FORM</h3>

	<form action="/onStudentReg" method="POST">
		<div class="card-content">
			<h4 style="color: #faae29;">Student Registration</h4><br>
			<div class="input-field">
				<input name="uid" onclick="putARHN()" id="uid" type="text" class="validate" required>
				<label class="active" for="uid">Aaorhan ID</label>
			</div>
			<div class="input-field">
				<input value="" id="t1" name="schoolId" type="text" class="validate" required>
				<label class="active" for="t1">School Id</label>
			</div>
			<div class="input-field">
				<input value="" id="t1" name="studentName" type="text" class="validate" required>
				<label class="active" for="t1">Student Name</label>
			</div>

			<div class="input-field">
				<select name="gender" required>
					<option disabled selected></option>
					<option>Male</option>
					<option>Female</option>
				</select>
				<label>Gender</label>
			</div>

			<div class="input-field">
				<select name="category" required>
					<option disabled selected></option>
					<option>Junior</option>
					<option>Senior</option>
				</select>
				<label>Category</label>
			</div>
			<div class="card-action">
				<button class="btn -waves-effect waves-light" type="submit" name="action">Submit
					<i class="material-icons right">send</i>
				</button>
			</div>
	</form>


</body>

</html>